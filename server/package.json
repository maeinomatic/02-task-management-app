    "@types/express": "^5.0.6",
    "@types/node": "^25.2.2",
    "@types/pg": "^8.16.0",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "pg": "^8.18.0",
    "prisma": "^7.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
