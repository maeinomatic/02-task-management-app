  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch src --ext ts --exec \"npm run build && node dist/index.js\"",
    "start": "node dist/index.js",
    "start:dev": "cross-env NODE_ENV=development USE_IN_MEMORY=false npm run dev",
    "start:dev_memory": "cross-env NODE_ENV=development USE_IN_MEMORY=true npm run dev",
    "start:test": "cross-env NODE_ENV=test USE_IN_MEMORY=false npm run start",
    "start:production": "cross-env NODE_ENV=production USE_IN_MEMORY=false npm run start",
    "prisma:generate": "npx prisma generate --schema=prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema=prisma/schema.prisma"
  },
